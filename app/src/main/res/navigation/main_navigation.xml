<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.sahrulhidayat.capstone.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/favoriteFragment"
        android:name="com.sahrulhidayat.favorite.ui.FavoriteFragment"
        app:moduleName="Capstone.favorite"
        android:label="fragment_favorite"
        tools:layout="@layout/fragment_favorite" />

    <include-dynamic
        android:id="@+id/dynamicFeatureFavorite"
        app:graphPackage="com.sahrulhidayat.favorite"
        app:graphResName="favorite_navigation"
        app:moduleName="Capstone.favorite" />

</navigation>